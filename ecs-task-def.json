{
    "family": "task",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "react1-container",
            "image": "435770184212.dkr.ecr.us-east-1.amazonaws.com/ecs:latest",
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}
